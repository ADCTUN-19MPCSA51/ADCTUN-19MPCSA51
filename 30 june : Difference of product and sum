class Solution {
    public static int subtractProductAndSum(int n)
	 { 
	int sum = 0; 
	int product = 1;
	 while (n > 0) 
	  { 
	    sum += n % 10;
	    product *= n % 10;
	    n /= 10; 
	  } 
	return product - sum;
	 }
    public static void main(String ar[])
	{
	Scanner s=new Scanner(System.in);
	 int n=s.nextInt();
	System.out.println(subtractProductAndSum(n));
 	 }
}

out put :
342
15
